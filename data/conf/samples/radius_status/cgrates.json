{
  
    "logger": {
        "level": 7
    },
    
    
    "listen": {
        "rpc_json": ":2012",				// RPC JSON listening address
        "rpc_gob": ":2013",					// RPC GOB listening address
        "http": ":2080",					// HTTP listening address
    },
    
    
    "db": {
        "db_conns": {
            "*default": {
                "db_type": "*internal",
                  "opts":{
            "internalDBRewriteInterval": "0s",
            "internalDBDumpInterval": "0s"
        }
            }
        }
    },
    
    "rals": {
        "enabled": true,
    },
    
    "schedulers": {
        "enabled": true,
    },
    
    "cdrs": {
        "enabled": true,
        "rals_conns": ["*internal"],
    },
    
    "resources": {
        "enabled": true,
        "store_interval": "-1",
    },
    
    "attributes": {
        "enabled": true,
    },
    
    "routes": {
        "enabled": true,
    },
    
    "chargers": {
        "enabled": true,
    },
    
    "sessions": {
        "enabled": true,
        "debit_interval": "10s",
        "conns":{
        "*attributes": [
                 {
                        "Tenant": "",
                        "FilterIDs": [],
                        "Values": ["*localhost"]
                 }
        ],
        "*cdrs": [
                 {
                        "Tenant": "",
                        "FilterIDs": [],
                        "Values": ["*localhost"]
                 }
        ],
        "*rals": [
                 {
                        "Tenant": "",
                        "FilterIDs": [],
                        "Values": ["*localhost"]
                 }
        ],
        "*resources": [
                 {
                        "Tenant": "",
                        "FilterIDs": [],
                        "Values": ["*localhost"]
                 }
        ],
        "*chargers": [
                 {
                        "Tenant": "",
                        "FilterIDs": [],
                        "Values": ["*internal"]
                 }
        ]
                        },
    },
    
    "radius_agent": {
        "enabled": true,
        "sessions_conns": ["*bijson_localhost"],
        "listeners": [
      {
        "network": "udp",
        "auth_address": "127.0.0.1:1812",
        "acct_address": "127.0.0.1:1813"
      }
    ],
        "client_dictionaries": {
            "*default": [ "/usr/share/cgrates/radius/dict/"]
        },
        "request_processors": [
            {
              "id": "Status",
              "filters": [
                "*string:~*vars.*radReqCode:StatusServer"
              ],
              "flags": [
                "*none","*log",
              ],
              "reply_fields": [
                {
                  "tag": "ReplyMessage",
                  "path": "*rep.Reply-Message",
                  "type": "*constant",
                  "mandatory": true,
                  "value": "OK",
                }
              ]
            }
          ]
    },
    
    
    
    "admins": {
        "enabled": true,
        "scheduler_conns": ["*internal"],
    },
    
    
    }
    