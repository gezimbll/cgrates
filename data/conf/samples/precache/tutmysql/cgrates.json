{
// CGRateS Configuration file
//

"general": {
	"reply_timeout": "30s",
},


"logger": {
	"level": 7
},


"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},

"data_db": {								// database used to store runtime data (eg: accounts, cdr stats)
	"db_type": "redis",						// data_db type: <redis|mongo>
	"db_port": 6379, 						// data_db port to reach the database
	"db_name": "10", 						// data_db database name to connect to
},

"stor_db": {
	"db_password": "CGRateS.org"
},

"caches":{
	"partitions": {
		"*resource_profiles": {"limit": 10000, "ttl":"", "precache": true,  "precache_filters": ["*prefix:~*req:RES_T","*prefix:~*req:RES_ACNT","*gt:~Limit:20"]}},
		"*resources": {"limit": 10000, "ttl":"", "precache": true},
		"*statqueues": {"limit": 10000, "ttl":"", "precache": true},
		"*statqueue_profiles": {"limit": 10000, "ttl":"", "precache": true},
		"*thresholds": {"limit": 10000, "ttl":"", "precache": true},
		"*threshold_profiles": {"limit": 10000, "ttl":"", "precache": true},
		"*filters": {"limit": 10000, "ttl":"", "precache": true},
		"*route_profiles": {"limit": 10000, "ttl":"", "precache": true, "precache_filters": ["*suffix:~*req:1"]}},
		"*attribute_profiles": {"limit": 10000, "ttl":"", "precache": true},
		"*resource_filter_indexes" :{"limit": 10000, "ttl":"", "precache": true},
		"*stat_filter_indexes" : {"limit": 10000, "ttl":"", "precache": true},
		"*threshold_filter_indexes" : {"limit": 10000, "ttl":"", "precache": true},
		"*route_filter_indexes" : {"limit": 10000, "ttl":"", "precache": true},
		"*attribute_filter_indexes" : {"limit": 10000, "ttl":"", "precache": true},
		"*charger_filter_indexes" : {"limit": 10000, "ttl":"", "precache": true},
	},
},

"migrator":{
	"users_filters":["Account"],
},


"admins": {
	"enabled": true,
},


}
