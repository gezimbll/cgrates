{
// CGRateS Configuration file
//
// Used for SessionSv1 integration tests


"general": {
	"reply_timeout": "10s"
},

"logger": {
	"level": 7
},

"listen": {
	"rpc_json": ":2012",
	"rpc_gob": ":2013",
	"http": ":2080",
},

"db": {
	"db_conns": {
		"*default": {
			"db_type": "*internal",
			"opts":{
				"internalDBRewriteInterval": "0s",
				"internalDBDumpInterval": "0s"
		}
    	}
	}
},

"rates": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"session_cost_retries": 1,
	"chargers_conns":["*internal"],
	"rates_conns": ["*internal"],
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
	"store_interval": "-1",
},


"attributes": {
	"enabled": true,
},


"thresholds": {
	"enabled": true,
	"store_interval": "-1",
},


"stats": {
	"enabled": true,
	"store_interval": "-1",
	"thresholds_conns": ["*internal"],
},


"routes": {
	"enabled": true,
},


"sessions": {
	"enabled": true,
	"session_ttl": "50ms",
	"alterable_fields": ["Extra1"],
	"conns":{
	"*chargers": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*rates": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*cdrs": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*resources": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*thresholds": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*stats": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*routes": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*attributes": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	]
			},
},


"admins": {
	"enabled": true,
},


}
