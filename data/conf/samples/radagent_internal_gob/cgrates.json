{
// CGRateS Configuration file
//

"logger": {
	"level": 7
},


"listen": {
	"rpc_json": ":2012",				// RPC JSON listening address
	"rpc_gob": ":2013",					// RPC GOB listening address
	"http": ":2080",					// HTTP listening address
},

"rpc_conns": {
	"conn1": {
		"strategy": "*first",
		"conns": [{"address": "127.0.0.1:2013", "transport":"*gob"}],
	},
},



"db": {
	"db_conns": {
		"*default": {
			"db_type": "*internal",	
				"opts":{
		"internalDBRewriteInterval": "0s",
		"internalDBDumpInterval": "0s"
	}
		},
	}
},




"rals": {
	"enabled": true,
},

"schedulers": {
	"enabled": true,
},

"cdrs": {
	"enabled": true,
	"rals_conns": ["*internal"],
},

"resources": {
	"enabled": true,
	"store_interval": "-1",
},

"attributes": {
	"enabled": true,
},

"routes": {
	"enabled": true,
},

"chargers": {
	"enabled": true,
},

"sessions": {
	"enabled": true,
	"debit_interval": "10s",
	"conns":{
	"*attributes": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["conn1"]
		 }
	],
	"*cdrs": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["conn1"]
		 }
	],
	"*rals": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["conn1"]
		 }
	],
	"*resources": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["conn1"]
		 }
	],
	"*chargers": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	]
			},
},

"radius_agent": {
	"enabled": true,
	"sessions_conns": ["conn1"],
},



"admins": {
	"enabled": true,
	"scheduler_conns": ["*internal"],
},


}
