			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*cdrs": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	],
	"*chargers": [
		 {
			"Tenant": "",
			"FilterIDs": [],
			"Values": ["*internal"]
		 }
	]
			},
},


"admins": {
	"enabled": true,
},


