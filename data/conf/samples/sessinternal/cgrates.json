		"*default": {
			"db_type": "*internal",
				"opts":{
		"internalDBRewriteInterval": "0s",
		"internalDBDumpInterval": "0s"
	}
    	}
	}
},

"rates": {
	"enabled": true,
},


"cdrs": {
	"enabled": true,
	"chargers_conns":["*internal"],
},


"attributes": {
	"enabled": true,
},


"chargers": {
	"enabled": true,
	"attributes_conns": ["*internal"],
},


"resources": {
	"enabled": true,
	"store_interval": "-1",
	"thresholds_conns": ["*internal"]
},


"stats": {
